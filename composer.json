{
  "name": "hardimpactdev/orbit-app",
  "description": "Web UI for Orbit",
  "keywords": [
    "hardimpactdev",
    "laravel",
    "orbit-app"
  ],
  "homepage": "https://github.com/hardimpactdev/orbit-app",
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Retel",
      "email": "nick@hardimpact.dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "illuminate/contracts": "^11.0||^12.0",
    "inertiajs/inertia-laravel": "^2.0",
    "laravel/mcp": "^0.5.2",
    "hardimpactdev/orbit-core": "^0.1"
  },
  "require-dev": {
    "larastan/larastan": "^3.0",
    "laravel/pint": "^1.25",
    "nunomaduro/collision": "^8.8",
    "orchestra/testbench": "^10.0.0||^9.0.0",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-arch": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "projektgopher/whisky": "^0.7.4",
    "spatie/laravel-ray": "^1.35"
  },
  "autoload": {
    "psr-4": {
      "HardImpact\\Orbit\\Ui\\": "src/",
      "HardImpact\\Orbit\\Ui\\Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HardImpact\\Orbit\\Ui\\Tests\\": "tests/",
      "Workbench\\App\\": "workbench/app/"
    }
  },
  "scripts": {
    "post-autoload-dump": "@composer run prepare",
    "post-install-cmd": [
      "@php -r 'if (!getenv(\"CI\")) { exec(\"vendor/bin/whisky update\"); }'"
    ],
    "post-update-cmd": [
      "@php -r 'if (!getenv(\"CI\")) { exec(\"vendor/bin/whisky update\"); }'"
    ],
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage --min=50",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HardImpact\\Orbit\\Ui\\OrbitAppServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "0.1.99-dev"
}
